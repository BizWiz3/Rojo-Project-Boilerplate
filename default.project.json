        "GlobalShadows": true,
        "TimeOfDay": "12:00:00",
        "ShadowSoftness": 0.2,
        "Technology": "ShadowMap",
        "ExposureCompensation": 0.0,
        "GeographicLatitude": 0,
        "ColorShift_Top": [0,0,0],
        "ColorShift_Bottom": [0,0,0]
